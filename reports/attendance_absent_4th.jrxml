	<parameter name="act_name" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select count(stud.`Stud_ID`) as count  from
tbl_section sec
inner join tbl_enrolldetails enroll on enroll.section_id = sec.section_id
inner join tbl_subject_events sube on sube.subject_id = sec.subject_id
inner join tbl_enrollees enrol on enrol.enrollee_id = enroll.enrollment_id
inner join tbl_student stud on stud.stud_id = enrol.stud_id
left outer join tbl_attendance as ab on ab.`Stud_ID` = stud.`Stud_ID` and ab.`ActivityID` = sube.`actID`
left outer join tbl_activities as act on act.activity_Id = sube.`actID`
where  act.act_name = $P{act_name} and enrol.current_status = 'Fourth Year']]>
	</queryString>
	<field name="count" class="java.lang.Long"/>
